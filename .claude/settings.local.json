{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(npx tsc:*)",
      "Bash(npm run lint)",
      "WebFetch(domain:supabase.com)",
      "WebFetch(domain:docs.github.com)"
    ],
    "deny": [
      "Bash(npx db push:*)",
      "Bash(db push:*)",
      "Bash(npm db push:*)",
      "Bash(npm run db:push:*)",
      "Bash(npx prisma db push:*)",
      "Bash(prisma db push:*)",
      "Bash(npm run db:push:*)",
      "Bash(npm run db:push:*)"
    ]
  }
}